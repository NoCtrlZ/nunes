<div id="admin-main">
<h2>News Manager</h2>
<%= link_to 'お知らせを作成', new_admin_news_path %>
<table>
<tr>
<td>お知らせタイトル</td>
<td>コマンド</td>
</tr>

<% @news.each do |news| %>
<tr>
<td><%= link_to news.title, admin_news_path(news.id) %></td>
<td><%= link_to "編集", edit_admin_news_path(news.id) %></td>
</tr>
<% end %>
</table>
</div>
